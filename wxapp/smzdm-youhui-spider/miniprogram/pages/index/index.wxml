<!--miniprogram/pages/index/index.wxml-->
<view class="page">
    <view class="youhuiItem" wx:for="{{youhuiLists}}" wx:key="{{index}}">
        <image src="{{item.img}}" />
        <view class="youhuiInfo">
            <view class="title">
                {{item.price}}
                <view class="price">
                    {{item.price}}
                </view>
            </view>
        </view>
        <view class="desc">
            {{item.desc}}
        </view>
    </view>
</view>